{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 4
  },
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false
  },
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！",
    "list_items": false
  },
  "MD029": {
    "style": "ordered"
  },
  "MD033": {
    "allowed_elements": ["br", "div", "span", "sub", "sup", "kbd", "details", "summary"],
    "allowed_attributes": ["id", "class", "style"],
    "allowed_classes": ["task-list-item"]
  },
  "MD034": false,
  "MD036": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD040": {
    "fenced_code_language": true,
    "allowed_languages": ["go", "bash", "sh", "yaml", "json", "markdown", "dockerfile", "sql"]
  },
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "code_blocks": true,
    "strings": true
  }
}